{
  "name": "accelerator-filesystem",
  "version": "1.0.0",
  "description": "Just a file system POC",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rm -rf docs/reports/unit-testing",
    "merge:reports": "mochawesome-merge docs/reports/unit-testing/*.json > cypress-tests-report.json",
    "create:html:report": "marge --reportDir docs/reports/unit-testing cypress-tests-report.json",
    "generate:html:report": "npm run merge:reports && npm run create:html:report",
    "test": "npm run clean:reports && npm run build && npm run cy:run && npm run generate:html:report",
    "lint": "eslint src --ext .js",
    "eslint-fix": "eslint src/**/*.js --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\"",
    "doc": "npx jsdoc -c ./jsDoc.json -t ./node_modules/ink-docstrap/template -R ./docs/README.md",
    "build": "npm run lint && npm run rm && npm run doc && babel src -d dist",
    "rm": "rm -rf ./dist && mkdir dist/",
    "start:dev": "npm stop && pm2-dev pm2.dev.config.js",
    "cy:run": "cypress run --spec \"cypress/integration/*\" --reporter mochawesome",
    "cy:open": "cypress open && cypress run",
    "start": "npm ci && npm run build && npm stop && pm2 start pm2.cluster.config.js",
    "stop": "pm2 stop pm2.cluster.config.js && pm2 delete pm2.cluster.config.js",
    "update": "pm2 update"
  },
  "author": "Jos√© Eduardo Perotta de Almeida",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.13.0",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@cypress/code-coverage": "^3.9.2",
    "assert": "^2.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "core-js": "^2.6.12",
    "cypress": "^6.4.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "prettier": "^2.2.1"
  },
  "presets": [
    "@babel/preset-env"
  ]
}
