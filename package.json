{
  "name": "accelerator-filesystem",
  "version": "1.0.0",
  "description": "Just a file system POC",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .js",
    "eslint-fix": "eslint src/**/*.js --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\"",
    "doc": "npx jsdoc -c ./jsDoc.json -t ./node_modules/ink-docstrap/template -R ./docs/README.md",
    "build": "npm run lint && npm run rm && npm run doc && babel src -d dist",
    "rm": "rm -rf ./dist && mkdir dist/",
    "start:dev": "pm2-dev pm2.dev.config.js",
    "pretest": "npm install",
    "test": "npm run build && cross-env PORT=8000 HOST=0.0.0.0 ./node_modules/.bin/mocha test --exit",
    "start": "npm ci && npm run build && cd dist && node index.js",
    "update": "pm2 update"
  },
  "author": "Jos√© Eduardo Perotta de Almeida",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.13.0",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "assert": "^2.0.0",
    "babel-node": "0.0.1-security",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.3.3",
    "chai-http": "^4.3.0",
    "core-js": "^2.6.12",
    "cross-env": "^7.0.2",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "prettier": "^2.2.1",
    "pm2": "^4.5.5"
  },
  "presets": [
    "@babel/preset-env"
  ]
}
